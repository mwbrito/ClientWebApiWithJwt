@model LoginResponse
@{ Layout = "_Layout"; ViewBag.Title = "Dev Info";}
 
<table class="table table-sm table-striped table-bordered m-2">
    <thead><tr><th>Usuário</th><th>Role</th><th>Token</th></tr></thead>
    <tbody>
        <tr>
            <td>@Model.user.Username</td>
            <td>@Model.user.Role</td>
            <td>@Model.Token</td>
        </tr>
    </tbody>
</table>

<form asp-controller="Home" asp-action="Authenticated" method="post">
    <input type="hidden" name="token" id="token" value="@Model.Token" />
    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Authenticated" >Verificar </a>
    <input type="submit" value="Verificar" />
</form>

